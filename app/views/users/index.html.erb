<% if current_user.has_role? :admin %>
  <div class="container">
    <div class="row">
      <h3>Users</h3>
      <div class="column">
        <table class="table">
          <tbody>
            <% @users.each do |user| %>
              <tr>
                <%= render user %>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
<% else %>
  <h1> You do not have access to this page </h1>
<% end %>
